# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next

class Solution:
    def hasCycle(self, head: Optional[ListNode]) -> bool:
        temp=head
        hs=set()
        while temp:
            if temp in hs:
                return True
            hs.add(temp)
            temp=temp.next
        return False
        
